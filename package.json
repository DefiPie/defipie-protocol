    "truffle-security": "^1.5.3"
  },
  "dependencies": {
    "@truffle/hdwallet-provider": "^1.3.1",
    "dotenv": "^10.0.0",
    "eth-saddle": "0.1.17",
    "truffle-flattener": "^1.5.0",
    "web3": "^1.3.4"
